{
  "rules": {
    "indent": 0,
    "max-len": 0,
    "no-useless-concat": 0,
    "one-var": 0,
    "space-before-function-paren": 0,
    "require-jsdoc": 0,
    "no-eq-null": 0,
    "eqeqeq": ["error", "allow-null"],
    "no-else-return": 0,
    "no-negated-condition": 0,
    "camelcase": 0,
    "no-lonely-if": 0,
    "no-implicit-coercion": 0,
    "wrap-iife": ["error", "any"],
    "no-trailing-spaces": ["error", { "skipBlankLines": true }],
    "max-statements-per-line": 0,
    "new-cap": 0,
    "object-curly-spacing": 0
  },
  "env": {
    "browser": true,
    "es6": true,
    "amd": true,
    "jasmine": true
  },
  "extends": "google"
}
